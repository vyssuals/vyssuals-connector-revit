<Window x:Class="Vyssuals.ConnectorRevit.VyssualsControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:local="clr-namespace:Vyssuals.ConnectorRevit"
        mc:Ignorable="d" 
        d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Button x:Name="stopButton" Content="Stop Plugin" Click="StopPlugin" Margin="10" VerticalAlignment="Top" />
        <Button x:Name="sendButton" Content="Send Data" Click="SendData" Margin="10" VerticalAlignment="Bottom" />
    </Grid>
</Window>
